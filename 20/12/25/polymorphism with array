class Animal {
    void sound() {
        System.out.println("Animal sound");
    }
}
class Cat extends Animal {
    void sound() {
        System.out.println("Meow");
    }
}
public class Main {
    public static void main(String[] args) {
        Animal[] a = { new Animal(), new Cat() };
        for (Animal x : a) {
            x.sound();
        }
    }
}
