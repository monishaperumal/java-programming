class PasswordStrength {
    public static void main(String[] args) {
        String p = "Java@123";
        int score = 0;

        if (p.length() >= 8) score++;
        if (p.matches(".*[A-Z].*")) score++;
        if (p.matches(".*[0-9].*")) score++;
        if (p.matches(".*[@#$!].*")) score++;

        System.out.println(score >= 3 ? "Strong" : "Weak");
    }
}
